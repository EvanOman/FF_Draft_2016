snakeFunction <- function(i)
{
groupIndicator <- ceiling(i / 8)
# Increasing group
if (groupIndicator %% 2 == 1)
{
i
}
# Decreasing group
else
{
(8*groupIndicator) - (((i - 1) %% 8) + 1) + 1
}
}
